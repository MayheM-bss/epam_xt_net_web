@using task_7._1._2_UI
@{
    if (IsPost)
    {
        var id = Request["editUserID"];
        var name = Request["newUserName"];
        var birth = Request["newUserBirth"];
        if(Guid.TryParse(id, out Guid guid))
        {
            Dependencies.UserManager.Edit(guid, name, birth);
            Response.Redirect("~/");
        }
    }
}

