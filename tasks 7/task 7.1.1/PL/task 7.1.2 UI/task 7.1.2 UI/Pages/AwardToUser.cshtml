@using task_7._1._2_UI;
@{
    if (IsPost)
    {
        var uID = Request["uID"];
        var aID = Request["aID"];
        if (Guid.TryParse(uID, out Guid userGuid) &&
            Guid.TryParse(aID, out Guid awardGuid))
        {
            Dependencies.UserManager.AddAward(userGuid, awardGuid);
            Response.Redirect("~/");
        }
    }
}

