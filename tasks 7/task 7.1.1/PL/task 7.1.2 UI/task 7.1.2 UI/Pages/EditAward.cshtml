@using task_7._1._2_UI
@{
    if (IsPost)
    {
        var id = Request["editAwardID"];
        var title = Request["newAwardTitle"];
        if(Guid.TryParse(id, out Guid guid))
        {
            Dependencies.AwardManager.Edit(guid, title);
            Response.Redirect("~/");
        }
    }
}
